TEST_MODE=1

# Service configuration
SERVICE_ENV=development
LOG_LEVEL=DEBUG
CORS_ORIGINS=["http://localhost"]

# DynamoDB Configuration
DYNAMODB_ENDPOINT=http://localhost:8000
AWS_REGION=us-east-1
DYNAMODB_TABLE=bg_readings

# RabbitMQ Configuration
RABBITMQ_URL=amqp://guest:guest@localhost:5672/

# Dexcom OAuth Configuration
DEXCOM_CLIENT_ID=
DEXCOM_CLIENT_SECRET=
DEXCOM_REDIRECT_URI=
DEXCOM_API_BASE_URL=  # Use sandbox for development

# Service Configuration
POLL_INTERVAL_SECONDS=900  # 15 minutes

METRICS_USER=your_metrics_username
METRICS_PASS=your_metrics_password

# KeyManager (local/dev)
KEYS_SECRET={"v1": "key1value", "v2": "key2value"}
CURRENT_KEY_VERSION=v2

# Example API key for get_secret
MY_API_KEY=supersecretapikeyvalue

# AWS (production)
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
AWS_DEFAULT_REGION=us-east-1
